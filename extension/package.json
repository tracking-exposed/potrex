{
  "name": "potrex-extension",
  "version": "0.4.5",
  "description": "potrex is the extension + backend for pornhub algorithm analysis. This is part of the https://tracking.exposed plan",
  "homepage": "https://github.com/tracking-exposed/potrex/",
  "bugs": "https://github.com/tracking-exposed/potrex/issues",
  "license": "GPL-3.0",
  "author": "Claudio Agosti, Giulia Corona, inherit from Alberto Granzotto: https://github.com/tracking-exposed/potrex/graphs/contributors ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/potrex.git"
  },
  "scripts": {
    "lint": "eslint ./",
    "build": "rimraf ./build && mkdir build && cross-env webpack --progress && cp -r src/popup/* manifest.json icons/build/* ./build/",
    "start": "npm run build:watch:files",
    "build:watch": "rimraf ./build && mkdir build && cp -r manifest.json icons/build/* src/popup/* ./build/ && cross-env webpack -w --progress",
    "build:dist": "sh pack-extension.sh",
    "clean": "rm -rf ./build ./dist",
    "test": "echo 'not really maintained' && mocha --compilers js:babel-register --reporter spec",
    "test:watch": "mocha -w --compilers js:babel-register --reporter spec",
    "docs": "rm -rf ./docs && ./build-docs.sh"
  },
  "dependencies": {
